# Example environment variables for KampusPay STTP

# Application
APP_NAME=KampusPay STTP
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# Prefix untuk kode pembayaran (mis. KP, STTP, FIN)
PAYMENT_CODE_PREFIX=KP

# Locale aplikasi (Indonesia)
APP_LOCALE=id
APP_FALLBACK_LOCALE=id

# Database (example for sqlite / change to mysql if needed)
DB_CONNECTION=sqlite

# Sanctum
SANCTUM_TOKEN_EXPIRATION=1440
SANCTUM_STATEFUL_DOMAINS=localhost,127.0.0.1,127.0.0.1:8000
SANCTUM_TOKEN_PREFIX=

# Default Passwords (override in production!)
ADMIN_DEFAULT_PASSWORD=ChangeThisAdminPass123!
KASIR_DEFAULT_PASSWORD=ChangeThisKasirPass123!
MAHASISWA_DEFAULT_PASSWORD=ChangeThisMahasiswaPass123!

# Cache TTLs (seconds)
SETTINGS_CACHE_TTL=600
TARIF_CACHE_TTL=600


